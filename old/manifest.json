{
    "name": "The Extension",
    "description": "A wonderful extension written in well-explained form",
    "version": "v1.0.0",
    "manifest_version": 2,
    "icons": { "16": "src/assets/icon16.png", "48": "src/assets/icon48.png", "128": "src/assets/icon128.png" },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": ["storage"]
}